// Code for fibonacci no's upto given number n.
#include<stdio.h>
#include<stdlib.h>
 int main(int argc,char* argv[])
 {
 int n,a=0,b=1,c,i;
 n=atoi(argv[1]);   // Taking the first command line argument variable into n(variable).
 for(i=1;i<=n;i++)
 {
 printf("%d ",a);
 c=a+b;
 a=b;
 b=c;
 }
 return 0;
 }
